class Pascal:
    def triangle(self, numRows):
        result = []

        for i in range(numRows):
            if i == 0:
                result.append([1])
            else:
                
                prev_row = result[-1]
                new_row = [1] + [prev_row[j] + prev_row[j + 1] for j in range(len(prev_row) - 1)] + [1]
                result.append(new_row)

        return result
sol = Pascal()
numRows = 5
result = sol.triangle(numRows)
print(result)


